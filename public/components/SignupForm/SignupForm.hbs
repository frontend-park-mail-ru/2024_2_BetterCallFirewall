<div class="cover" data-key="{{this.key}}">
    <div class="signup">
        <div class="title-sign"><a href="/feed" class="title-link"><h1>Vilka</h1></a></div>
        <div class="name-sign"><h2>Регистрация</h2></div>
        <div class="form-sign">
            <form action="post" class="{{this.className}}" id="{{this.id}}">
                {{#each inputs}}
                    {{{this}}}
                {{/each}}
                <span class="error-message"></span>
                {{{button}}}
                {{{toLoginLink}}}
            </form>
        </div>
    </div>
</div>
