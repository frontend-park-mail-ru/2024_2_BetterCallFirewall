<div class="change-password-form" data-key="{{this.key}}">
    <h2 class="change-password-form__title">Сменить пароль</h2>
    <form method="post" class="form" id="formChangePassword" data-key="form">
        {{#each textAreas}}
            {{{this}}}
        {{/each}}
        {{#each inputs}}
            {{{this}}}
        {{/each}}
        <span class="form__error">
            {{{error}}}
        </span>
        {{{button}}}
    </form>
</div>
